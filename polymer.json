{
  "entrypoint": "index.html",
  "shell": "src/ft-test.html",
  "fragments": [
  ],
  "sources": [
    "src/**/*",
    "robots.txt"
  ],
  "extraDependencies": [
    "manifest.json"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ],
    "ignoreWarnings": [
      "multiple-doc-comments"
    ]
  },
  "builds": [
    {
      "name": "dev",
      "bundle": false,
      "js": {"minify": false, "compile": false},
      "css": {"minify": false},
      "html": {"minify": false},
      "addServiceWorker": false,
      "addPushManifest": false
    },
    {
      "name": "prod",
      "preset": "es5-bundled"
    },
    {
      "name": "debug",
      "preset": "es5-bundled",
      "bundle": false,
      "js": { "minify": false},
      "css": {"minify": false},
      "html": { "minify": false},
      "addServiceWorker": false
    }
  ]
}